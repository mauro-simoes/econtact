{% extends 'base.html.twig' %}

{% block title %}Contacts{% endblock %}

{% block body %}
    <h1>Contacts</h1>
    <table>
        <thead>
            <tr>
                <th>ID Contact</th>
                <th>ID Nom</th>
            </tr>
        </thead>
        <tbody>
            {% for contact in contacts %}
                <tr>
                    <td>{{ contact.getIdNom() }}</td>
                    <td>{{ contact.getIdContact() }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
    <h3> Ajout d'un contact </h3> 
{% endblock %}
